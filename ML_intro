import matplotlib.pyplot as plt
import numpy as np
from scipy import stats

x=[5,7,12,13,18]
y=[1,12,16,21,23]

plt.scatter(x,y)
plt.show()

slope, intercept,r ,p ,std_err = stats.linregress(x,y)
def myfunc(x):
  return slope * x + intercept

mymodel=list(map(myfunc, x))
plt.scatter(x, y)
plt.plot(x, mymodel)
plt.show()

print(r)
print(p)

